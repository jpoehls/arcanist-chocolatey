    <bugTrackerUrl>https://secure.phabricator.com</bugTrackerUrl>
    <projectSourceUrl>https://secure.phabricator.com/diffusion/ARC</projectSourceUrl>
    <tags>phabricator arcanist admin</tags>
    <copyright>
    </copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
